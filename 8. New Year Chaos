#include <bits/stdc++.h>
using namespace std;

int main() {
    ios::sync_with_stdio(false);
    cin.tie(nullptr);

    int t;
    cin >> t;
    while (t--) {
        int n;
        cin >> n;
        vector<int> q(n);
        for (int i = 0; i < n; i++) cin >> q[i];

        int bribes = 0;
        bool chaotic = false;

        for (int i = 0; i < n; i++) {
            if (q[i] - (i + 1) > 2) {
                chaotic = true;
                break;
            }
            for (int j = max(0, q[i] - 2); j < i; j++)
                if (q[j] > q[i]) bribes++;
        }

        if (chaotic) cout << "Too chaotic\n";
        else cout << bribes << "\n";
    }
}
