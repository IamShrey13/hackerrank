#include <bits/stdc++.h>
using namespace std;

vector<int> icecreamParlor(int m, vector<int> cost) {
    unordered_map<int,int> mp;
    vector<int> result(2);

    for(int i=0; i<cost.size(); i++) {
        int complement = m - cost[i];
        if(mp.find(complement) != mp.end()) {
            result[0] = mp[complement] + 1;
            result[1] = i + 1;
            break;
        }
        mp[cost[i]] = i;
    }

    sort(result.begin(), result.end());
    return result;
}

int main() {
    int t;
    cin >> t;
    for(int a0 = 0; a0 < t; a0++) {
        int m, n;
        cin >> m >> n;
        vector<int> cost(n);
        for(int i = 0; i < n; i++) {
            cin >> cost[i];
        }
        vector<int> res = icecreamParlor(m, cost);
        cout << res[0] << " " << res[1] << "\n";
    }
    return 0;
}
