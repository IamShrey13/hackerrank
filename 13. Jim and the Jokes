#include <bits/stdc++.h>
using namespace std;

bool isValid(int month, int day) {
    int temp = day;
    while(temp > 0) {
        if(temp % 10 >= month) return false;
        temp /= 10;
    }
    return true;
}

int toDecimal(int month, int day) {
    int result = 0;
    int multiplier = 1;
    while(day > 0) {
        result += (day % 10) * multiplier;
        multiplier *= month;
        day /= 10;
    }
    return result;
}

int main() {
    int n;
    cin >> n;
    unordered_map<int,int> freq;
    
    for(int i=0; i<n; i++) {
        int m,d;
        cin >> m >> d;
        if(!isValid(m,d)) continue;
        int val = toDecimal(m,d);
        freq[val]++;
    }

    long long jokes = 0;
    for(auto it : freq) {
        int f = it.second;
        jokes += (long long)f*(f-1)/2;
    }

    cout << jokes << "\n";
    return 0;
}
