#include <bits/stdc++.h>
using namespace std;

bool isValid(int month, int day) {
    int temp = day;
    while(temp > 0) {
        if(temp % 10 >= month) return false;
        temp /= 10;
    }
    return true;
}

int toDecimal(int month, int day) {
    int result = 0;
    int multiplier = 1;
    while(day > 0) {
        result += (day % 10) * multiplier;
        multiplier *= month;
        day /= 10;
    }
    return result;
}

int main() {
    int n;
    cin >> n;
    vector<pair<int,int>> events(n);
    for(int i=0; i<n; i++) {
        cin >> events[i].first >> events[i].second;
    }

    int jokes = 0;
    for(int i=0; i<n; i++) {
        int m1 = events[i].first, d1 = events[i].second;
        if(!isValid(m1,d1)) continue;

        int val1 = toDecimal(m1,d1);
        for(int j=i+1; j<n; j++) {
            int m2 = events[j].first, d2 = events[j].second;
            if(!isValid(m2,d2)) continue;

            int val2 = toDecimal(m2,d2);
            if(val1 == val2) jokes++;
        }
    }

    cout << jokes << "\n";
    return 0;
}
