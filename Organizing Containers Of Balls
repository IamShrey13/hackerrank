#include <bits/stdc++.h>
using namespace std;
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    int q; 
    cin>>q;
    while(q--){
        int n;
        cin>>n;
        vector<vector<long long>> M(n,vector<long long>(n));
        vector<long long>container(n,0),type(n,0);
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                cin>>M[i][j];
                container[i]+=M[i][j];
                type[j]+=M[i][j];
            }
        }
        sort(container.begin(),container.end());
        sort(type.begin(),type.end());
        if(container==type) 
            cout<<"Possible\n";
        else 
            cout<<"Impossible\n";
    }
    return 0;
}
