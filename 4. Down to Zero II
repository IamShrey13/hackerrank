#include <bits/stdc++.h>
using namespace std;
int main(){
    ios::sync_with_stdio(false);
    cin.tie(nullptr);
    const int MAXN=1000000;
    vector<int>dp(MAXN+1,INT_MAX);
    dp[0]=0;
    dp[1]=1;
    for(int i=2;i<=MAXN;i++){
        dp[i]=dp[i-1]+1;
        for(int j=2;j*j<=i;j++){
            if (i%j==0){
                int x=max(j,i/j);
                dp[i]=min(dp[i],dp[x]+1);
            }
        }
    }
    int q;
    cin>>q;
    while(q--){
        int n;
        cin>>n;
        cout<<dp[n]<<"\n";
    }
    return 0;
}
